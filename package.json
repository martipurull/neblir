{
  "name": "neblir",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev | pino-pretty",
    "dev:raw": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:file": "vitest --run",
    "test:typecheck": "tsc -p tsconfig.test.json --noEmit",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "prisma:push": "npx prisma db push",
    "prisma:generate": "npx prisma generate",
    "prepare": "husky",
    "push:pr": "git push -u origin HEAD && (gh pr view --json url -q .url || (gh pr create --fill >/dev/null && gh pr view --json url -q .url))"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.937.0",
    "@aws-sdk/s3-request-presigner": "^3.937.0",
    "@prisma/client": "^6.5.0",
    "csv-parse": "^6.1.0",
    "dotenv": "^16.4.7",
    "googleapis": "^169.0.0",
    "mongodb": "^6.15.0",
    "next": "^15.2.1",
    "next-auth": "^5.0.0-beta.25",
    "node": "^20.19.0",
    "pino": "^10.1.0",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.53.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@testing-library/react": "^16.0.0",
    "@types/csv-parse": "^1.1.12",
    "@types/node": "^20.17.24",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "cypress": "^13.13.0",
    "eslint": "^8",
    "eslint-config-next": "^15.5.12",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "pino-pretty": "^13.1.2",
    "postcss": "^8",
    "prettier": "^3.6.2",
    "prisma": "^6.5.0",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2",
    "vitest": "^3.0.8"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  }
}
